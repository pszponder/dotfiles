        "rules": [
          {
            "description": "Caps Lock to Cmd when held, Escape when tapped",
            "manipulators": [
              {
                "type": "basic",
                "from": {
                  "key_code": "caps_lock",
                  "modifiers": {
                    "optional": ["any"]
                  }
                },
                "to": [
                  {
                    "key_code": "left_command"
                  }
                ],
                "to_if_alone": [
                  {
                    "key_code": "escape"
                  }
                ]
              }
            ]
          },
          {
            "description": "Shift + Spacebar + HJKL to Home/PageDown/PageUp/End",
            "manipulators": [
              {
                "type": "basic",
                "from": {
                  "key_code": "h",
                  "modifiers": {
                    "mandatory": ["shift"]
                  }
                },
                "to": [
                  {
                    "key_code": "home"
                  }
                ],
                "conditions": [
                  {
                    "type": "variable_if",
                    "name": "spacebar_held",
                    "value": 1
                  }
                ]
              },
              {
                "type": "basic",
                "from": {
                  "key_code": "j",
                  "modifiers": {
                    "mandatory": ["shift"]
                  }
